{
    "name": "spotify-analytics-api",
    "version": "0.0.1",
    "main": "src/main.ts",
    "repository": "git@github.com:TheIthorian/spotify-analytics.git",
    "author": "TheIthorian",
    "license": "Apache-2.0",
    "scripts": {
        "ts": "ts-node --files",
        "build": "yarn tsc",
        "start": "yarn ts src/main.ts",
        "start:jobs": "ts-node src/upload/fileProcessor.ts",
        "dev": "yarn ts-node-dev src/main.ts | pino-pretty",
        "dev:jobs": " ts-node-dev src/jobs/fileProcessor.ts | pino-pretty",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:cov": "jest --coverage",
        "test:e2e": "jest --config jest.json"
    },
    "lint-staged": {
        "*.ts": [
            "eslint",
            "prettier --write"
        ]
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.test\\.(t|j)s$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.1",
        "@commitlint/config-conventional": "^17.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^28.1.6",
        "@types/md5": "^2.3.2",
        "@types/node": "^18.6.5",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.26.0",
        "@typescript-eslint/parser": "^5.26.0",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.3",
        "jest": "^28.1.0",
        "jest-mock-extended": "^3.0.4",
        "lint-staged": "^13.0.3",
        "md5": "^2.3.0",
        "pino-pretty": "^10.0.0",
        "prettier": "^2.3.0",
        "supertest": "^6.1.3",
        "ts-jest": "^28.0.3",
        "ts-node": "^10.8.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "@prisma/client": "^4.12.0",
        "@types/express-fileupload": "^1.4.1",
        "@types/node-fetch": "^2.6.3",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-fileupload": "^1.4.0",
        "express-promise-router": "^4.1.1",
        "node-cron": "^3.0.2",
        "node-fetch": "^2.6.9",
        "pino": "^8.11.0",
        "pino-multi-stream": "^6.0.0",
        "prisma": "^4.12.0",
        "zod": "^3.21.4"
    }
}
